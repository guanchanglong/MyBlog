<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gcl.demo1.dao.mybatis.MTagDao">

    <select id="findTagByBlogId" resultType="com.gcl.demo1.entity.mybatis.Tag">
        select * from t_tag where id = (select tags_id from t_blog_tags where blogs_id = #{blogId})
    </select>

</mapper>